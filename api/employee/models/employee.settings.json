{
  "kind": "collectionType",
  "collectionName": "employees",
  "info": {
    "name": "Employee",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Employee_ID": {
      "type": "biginteger",
      "required": true
    },
    "First_Name": {
      "type": "string",
      "required": true,
      "maxLength": 25
    },
    "Last_Name": {
      "type": "string",
      "required": true,
      "maxLength": 25
    },
    "Email": {
      "type": "string",
      "required": true,
      "maxLength": 25
    },
    "Phone_Number": {
      "type": "string",
      "required": true,
      "maxLength": 20,
      "unique": true
    },
    "Hire_Date": {
      "type": "date",
      "required": true
    },
    "Job_ID": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 10
    },
    "Salary": {
      "type": "decimal",
      "required": true
    },
    "Commission_PCT": {
      "type": "decimal",
      "required": true
    },
    "Manager_ID": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "Department_ID": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "job_histories": {
      "via": "employee",
      "collection": "job-history"
    },
    "department": {
      "via": "employees",
      "model": "department"
    },
    "job": {
      "via": "employees",
      "model": "job"
    }
  }
}
